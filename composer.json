{
  "name": "bemcasei/whois-module",
  "description": "ZF3 Module to query whois.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "William Hoffmann",
      "email": "williamhoffmann@outlook.com",
      "role": "Developer"
    }
